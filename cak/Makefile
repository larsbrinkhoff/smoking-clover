  SYS_LIBRARIES = $(XTOOLLIB) $(XMULIB) $(XLIB)
LOCAL_LIBRARIES =
           SRCS = clover.c
           OBJS = clover.o

 PROGRAM = clover

all:: clover

clover: $(OBJS) $(LOCAL_LIBRARIES)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LOCAL_LIBRARIES) $(LDFLAGS) $(SYSLAST_LIBRARIES)

relink::
	$(RM) $(PROGRAM)
	$(MAKE) $(MFLAGS) $(PROGRAM)

install:: clover
	$(INSTALL) -c $(INSTALLFLAGS) clover $(BINDIR)

install.man:: clover.man
	$(INSTALL) -c $(INSTMANFLAGS) clover.man $(MANDIR)/clover.n

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" -- $(CFLAGS) -- $(SRCS)

$(DEPEND):
	@echo "making $@"; \
	cd $(DEPENDSRC); $(MAKE)

clean::
	$(RM) $(PROGRAM)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

